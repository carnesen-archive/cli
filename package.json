{
  "name": "alwaysai",
  "description": "The alwaysAI command-line interface (CLI)",
  "version": "0.0.0-19",
  "main": "lib/index.js",
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "types": "lib/index.d.ts",
  "bin": {
    "alwaysai": "lib/index.js"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "alwaysai-build && tsc --project tsconfig.build.json",
    "build:clean": "rimraf lib && npm run build",
    "build:watch": "tsc --project . --watch",
    "lint": "tslint --project tsconfig.build.json",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run lint && npm run test:unit && npm run build",
    "test:unit": "jest --coverage src",
    "test:watch": "npm run test:unit -- --watch",
    "preversion": "npm test && npm run build:clean",
    "postversion": "npm publish"
  },
  "dependencies": {
    "@alwaysai/always-cli": "0.0.0-11",
    "@alwaysai/cloud-api": "0.0.0-11",
    "@alwaysai/cloud-api-nodejs": "0.0.0-8",
    "@alwaysai/codecs": "0.0.0-5",
    "@alwaysai/config-nodejs": "0.0.0-4",
    "@carnesen/coded-error": "0.1.0",
    "@types/download": "6.2.4",
    "@types/lodash.difference": "4.5.6",
    "@types/mkdirp": "0.5.2",
    "@types/prompts": "1.2.0",
    "@types/remote-origin-url": "2.0.0",
    "@types/rimraf": "2.0.2",
    "@types/ssh2": "0.5.38",
    "@types/tar": "4.0.0",
    "chalk": "2.4.2",
    "download": "7.1.0",
    "firebase": "5.10.1",
    "http-status-codes": "1.3.2",
    "io-ts": "1.8.5",
    "lodash.difference": "4.5.0",
    "mkdirp": "0.5.1",
    "ora": "3.4.0",
    "prompts": "2.0.4",
    "remote-origin-url": "2.0.0",
    "rimraf": "2.6.3",
    "ssh2": "0.8.4",
    "tar": "4.4.8"
  },
  "devDependencies": {
    "@alwaysai/build-cli": "0.0.0-2",
    "@alwaysai/tsconfig": "0.0.0",
    "@alwaysai/tslint-config": "0.0.3",
    "@types/jest": "24.0.12",
    "@types/node": "11.13.8",
    "jest": "24.7.1",
    "tempy": "0.3.0",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tslint": "5.16.0",
    "typescript": "3.4.5"
  },
  "files": [
    "src",
    "lib",
    "assets",
    "alwaysai.git.json"
  ],
  "keywords": [
    "alwaysai",
    "typescript",
    "nodejs",
    "cli",
    "command",
    "line",
    "interface"
  ],
  "author": {
    "name": "alwaysAI, Inc.",
    "email": "contact@alwaysai.co"
  },
  "repository": "github:alwaysai/cli",
  "homepage": "https://github.com/alwaysai/cli#readme",
  "license": "MIT"
}
