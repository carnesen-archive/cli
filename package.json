{
  "name": "alwaysai",
  "description": "The alwaysAI command-line interface (CLI)",
  "version": "0.0.0-4",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "alwaysai": "lib/index.js"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "build:clean": "rimraf lib && npm run build",
    "build:watch": "tsc --project . --watch",
    "lint": "tslint --project tsconfig.build.json",
    "lint:fix": "npm run lint -- --fix",
    "test": "npm run lint && npm run test:unit && npm run build",
    "test:unit": "jest --coverage src",
    "test:watch": "npm run test:unit -- --watch",
    "preversion": "npm test && npm run build:clean",
    "postversion": "npm publish"
  },
  "dependencies": {
    "@alwaysai/always-cli": "0.0.0-6",
    "@alwaysai/cloud-api": "0.0.0-4",
    "@alwaysai/cloud-api-nodejs": "0.0.0-4",
    "@carnesen/coded-error": "0.1.0",
    "@types/lodash.difference": "4.5.6",
    "@types/mkdirp": "0.5.2",
    "@types/prompts": "1.2.0",
    "@types/remote-origin-url": "2.0.0",
    "@types/ssh2": "0.5.38",
    "@types/tar": "4.0.0",
    "http-status-codes": "1.3.2",
    "lodash.difference": "4.5.0",
    "mkdirp": "0.5.1",
    "ora": "3.3.0",
    "prompts": "2.0.4",
    "remote-origin-url": "2.0.0",
    "ssh2": "0.8.2",
    "tar": "4.4.8"
  },
  "devDependencies": {
    "@alwaysai/tsconfig": "0.0.0",
    "@alwaysai/tslint-config": "0.0.2",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.0",
    "@types/tempy": "0.2.0",
    "jest": "24.6.0",
    "rimraf": "2.6.3",
    "tempy": "0.2.1",
    "ts-jest": "24.0.1",
    "ts-node": "8.0.3",
    "tslint": "5.15.0",
    "typescript": "3.4.1"
  },
  "files": [
    "src",
    "lib",
    "assets"
  ],
  "keywords": [
    "alwaysai",
    "typescript",
    "nodejs",
    "cli",
    "command",
    "line",
    "interface"
  ],
  "author": {
    "name": "alwaysAI, Inc.",
    "email": "contact@alwaysai.co"
  },
  "repository": "github:alwaysai/cli",
  "homepage": "https://github.com/alwaysai/cli#readme",
  "license": "MIT"
}
