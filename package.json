{
  "name": "alwaysai",
  "description": "The alwaysAI command-line interface (CLI)",
  "version": "0.4.0",
  "main": "lib/index.js",
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "types": "lib/index.d.ts",
  "bin": {
    "aai": "lib/index.js",
    "alwaysai": "lib/index.js"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "alwaysai-build && tsc --project tsconfig.build.json",
    "build:clean": "rimraf lib && npm run build",
    "build:watch": "tsc --project . --watch",
    "lint": "tslint --project tsconfig.build.json",
    "lint:fix": "npm run lint -- --fix",
    "test": "./test.sh",
    "test:ci": "./test.sh --ci",
    "test:unit": "jest --coverage src",
    "test:watch": "npm run test:unit -- --watch",
    "preversion": "npm test && npm run build:clean",
    "postversion": "npm publish"
  },
  "dependencies": {
    "@alwaysai/alwayscli": "0.1.0",
    "@alwaysai/cloud-api": "0.1.1",
    "@alwaysai/codecs": "0.0.0",
    "@alwaysai/config-nodejs": "0.0.1",
    "@carnesen/coded-error": "0.1.0",
    "@types/download": "6.2.4",
    "@types/keymirror": "0.1.1",
    "@types/lodash.difference": "4.5.6",
    "@types/mkdirp": "0.5.2",
    "@types/node-fetch": "2.5.2",
    "@types/prompts": "2.4.0",
    "@types/pump": "1.1.0",
    "@types/rimraf": "2.0.2",
    "@types/semver": "6.0.2",
    "@types/signal-exit": "3.0.0",
    "@types/tar": "4.0.3",
    "@types/yauzl": "2.9.1",
    "chalk": "2.4.2",
    "delay": "4.3.0",
    "download": "7.1.0",
    "fp-ts": "2.0.5",
    "http-status-codes": "1.3.2",
    "io-ts": "2.0.1",
    "keymirror": "0.1.1",
    "lodash.difference": "4.5.0",
    "log-symbols": "3.0.0",
    "mkdirp": "0.5.1",
    "node-fetch": "2.6.0",
    "ora": "4.0.2",
    "prompts": "2.2.1",
    "pump": "3.0.0",
    "rimraf": "3.0.0",
    "semver": "6.3.0",
    "signal-exit": "3.0.2",
    "tar": "5.0.2",
    "tempy": "0.3.0",
    "to-readable-stream": "2.1.0",
    "yauzl": "2.10.0"
  },
  "devDependencies": {
    "@alwaysai/build-cli": "0.0.0",
    "@alwaysai/tsconfig": "0.0.0",
    "@alwaysai/tslint-config": "0.0.3",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.8",
    "get-stream": "5.1.0",
    "jest": "24.9.0",
    "ts-jest": "24.1.0",
    "ts-node": "8.4.1",
    "tslint": "5.20.0",
    "typescript": "3.6.3"
  },
  "files": [
    "src",
    "lib",
    "assets",
    "alwaysai.git.json",
    "package-lock.json"
  ],
  "keywords": [
    "alwaysai",
    "typescript",
    "nodejs",
    "cli",
    "command",
    "line",
    "interface"
  ],
  "author": {
    "name": "alwaysAI, Inc.",
    "email": "contact@alwaysai.co"
  },
  "repository": "github:alwaysai/cli",
  "homepage": "https://github.com/alwaysai/cli#readme",
